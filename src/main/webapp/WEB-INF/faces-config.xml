<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee         http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>/airline.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>airlineList</from-outcome>
			<to-view-id>/airlineList</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>airlineList</from-outcome>
			<to-view-id>/airlineList.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/airlineList.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/airline</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editForm</from-outcome>
			<to-view-id>/airline</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>airlineList</from-outcome>
			<to-view-id>/airlineList.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>delete</from-outcome>
			<to-view-id>/airlineList</to-view-id>
		</navigation-case>
	</navigation-rule>

	<application />
</faces-config>
